<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.alkautsarefendi.dashboardtkr">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".activity.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.LoginActivity" />
        <activity
            android:name=".activity.HalamanUtamaActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".activity.DashboardActivity" />
        <activity android:name=".activity.LoketActivity" />
        <activity android:name=".activity.WilayahActivity" />
        <activity android:name=".activity.HarianActivity" />
        <activity android:name=".activity.BacaMeterActivity" />
        <activity android:name=".activity.GantiMeterActivity" />
        <activity android:name=".activity.PasangBaruActivity" />
        <activity android:name=".activity.PasangBaruSummaryActivity" />
        <activity android:name=".activity.PenyambunganActivity" />
        <activity android:name=".activity.PemutusanActivity"></activity>
    </application>

</manifest>